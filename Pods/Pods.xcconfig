CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/ChameleonFramework" "${PODS_ROOT}/Headers/Tweaks" "${PODS_ROOT}/Headers/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/ChameleonFramework" -isystem "${PODS_ROOT}/Headers/Tweaks" -isystem "${PODS_ROOT}/Headers/pop"
OTHER_LDFLAGS = -ObjC -lc++ -framework CoreGraphics -framework MessageUI -framework QuartzCore
PODS_ROOT = ${SRCROOT}/Pods